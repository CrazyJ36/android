<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:text="lt"
        android:id="@+id/leftTopButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/circle_button"
        android:onClick="onClickLeftTopComplication"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintWidth_percent="0.21"
        app:layout_constraintHeight_percent="0.21"
        app:layout_constraintHorizontal_bias="0.319"
        app:layout_constraintVertical_bias="0.216"/>
    <Button
        android:text="lb"
        android:id="@+id/leftBottomButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/circle_button"
        android:onClick="onClickLeftBottomComplication"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintWidth_percent="0.21"
        app:layout_constraintHeight_percent="0.21"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintVertical_bias="0.48"/>
    <Button
        android:text="rt"
        android:id="@+id/RightTopButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/circle_button"
        android:onClick="onClickRightTopComplication"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintWidth_percent="0.21"
        app:layout_constraintHeight_percent="0.21"
        app:layout_constraintHorizontal_bias="0.68"
        app:layout_constraintVertical_bias="0.20"/>
    <Button
        android:text="rb"
        android:id="@+id/rightBottomButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/circle_button"
        android:onClick="onClickRightBottomComplication"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintWidth_percent="0.21"
        app:layout_constraintHeight_percent="0.21"
        app:layout_constraintHorizontal_bias="0.68"
        app:layout_constraintVertical_bias="0.48"/>
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/editorPreviewContentDescription" />
</androidx.constraintlayout.widget.ConstraintLayout>