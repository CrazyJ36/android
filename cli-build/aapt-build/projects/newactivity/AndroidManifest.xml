	android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="1" android:targetSdkVersion="29" />

    <application
        android:label="@string/appname"
        android:theme="@android:style/Theme.DeviceDefault" >

		<activity
			android:name=".MainActivity">
